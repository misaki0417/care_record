利用者新規登録
<%= form_with model:@user, url:users_path do |user| %>
<div class="container">
  <div class="row">
    <div class="col-md-4">
      <table>
        <tr>
          <th>氏名</th>
          <td><%= user.text_field :user_name %></td>
        </tr>
        <tr>
          <th>生年月日</th>
          <td><%= user.date_field :birthdate %></td>
        </tr>
        <tr>
          <th>プロフィール</th>
          <td><%= user.text_area :profile %></td>
        </tr>
      </table>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <%= user.submit "登録する" %>
    </div>
  </div>

</div>

<% end %>


